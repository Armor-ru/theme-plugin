{
  "version": "1.0.29",
  "license": "UNLICENSED",
  "main": "index.js",
  "typings": "index.d.ts",
  "files": [
    "nuxt",
    "utils",
    "vite"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc && npm run build:plugin-nuxt && npm run build:vite && cp package*.json dist",
    "build:plugin-nuxt": "rollup dist/nuxt/plugin.js --file dist/nuxt/plugin.js --format cjs",
    "build:vite": "rollup dist/vite/index.js --file dist/vite/index.js --format cjs",
    "pub": "npm run build && npm publish ./dist"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@armor-ru/theme",
  "author": "wiwishka",
  "module": "dist/vite.esm.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/Armor-ru/theme-plugin",
  "dependencies": {
    "hash-sum": "^2.0.0",
    "query-ast": "^1.0.5",
    "scss-parser": "^1.0.6"
  },
  "devDependencies": {
    "@nuxt/types": "^2.15.8",
    "@types/hash-sum": "^1.0.0",
    "@types/scss-parser": "^1.0.1",
    "husky": "^8.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3"
  }
}
